
<main class="flex-grow-1 d-flex w-100 flex-wrap justify-content-evenly">
  <section
    class="w-50 mb-4 d-flex flex-column justify-content-start align-items-center"
    style="min-width: 380px;"
  >
    <h2 class="mt-5 mb-2 text-center">REGISTRAR USUARIO</h2>
    <br>
    <form
      style="max-width: 360px;"
      class="w-100 d-flex flex-column justify-content-center items-align-center"
    >
      <fieldset class="mb-3 text-center">
        <label for="title" class="form-label mb-0">Nombre usuario</label>
        <input type="text" class="form-control" id="title" />
      </fieldset>
      <fieldset class="mb-3 text-center">
        <label for="photo" class="form-label mb-0">Imagen</label>
        <input type="text" class="form-control" id="photo" />
      </fieldset>
      <fieldset class="mb-3 text-center">
        <label for="price" class="form-label mb-0">Email</label>
        <input type="number" class="form-control" id="price" />
      </fieldset>
     
  
      <button
        id="newProduct"
        type="button"
        class="w-100 btn btn-danger"
      >CREATE!</button>
    </form>

</main>
<script src="/socket.io/socket.io.js"></script>
<script src="/real.js"></script>