
<main class="flex-grow-1 d-flex w-100 flex-wrap justify-content-evenly">
  <section
    class="w-50 mb-4 d-flex flex-column justify-content-start align-items-center"
    style="min-width: 380px;"
  >
    <h2 class="mt-5 mb-2 text-center">LOG IN!</h2>
    <br>
    <form
      style="max-width: 360px;"
      class="w-100 d-flex flex-column justify-content-center items-align-center"
    >
      <fieldset class="mb-3 text-center">
        <label for="email" class="form-label mb-0">Email</label>
        <input type="text" class="form-control" id="email" />
      </fieldset>
      <fieldset class="mb-3 text-center">
        <label for="password" class="form-label mb-0">Password</label>
        <input type="password" class="form-control" id="password" />
      </fieldset>
     
  
      <button
        id="login"
        type="button"
        class="w-100 btn btn-danger"
      >Log in!</button>
      <br>
      <button
        id="google"
        type="button"
        class="w-100 btn btn-danger">
         <a href="http://localhost:8080/api/sessions/google" style="color: white; text-decoration: none;">Google!</a>
      </button>
      <br>
       <button
        id="github"
        type="button"
        class="w-100 btn btn-danger"
      >Github!</button>
    </form>

</main>
<script src="/socket.io/socket.io.js"></script>
<script src="/scripts/login.js"></script>
<script src="/scripts/third.js"></script>